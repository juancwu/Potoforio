{
    "name": "potoforio",
    "version": "1.0.0",
    "description": "JC's personal portfolio site",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"pnpm run tw:dev\" \"air\"",
        "tw:dev": "tailwindcss -i ./styles.css -o ./static/styles.css --watch",
        "tw:prod": "tailwindcss -i ./styles.css -o ./static/styles.css --minify",
        "prettier": "prettier ./public --write",
        "prettier:check": "prettier ./public --check"
    },
    "keywords": [
        "personal",
        "porfolio",
        "projects",
        "resume"
    ],
    "author": "JC",
    "license": "MIT",
    "devDependencies": {
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.2",
        "postcss": "^8.4.31",
        "prettier-plugin-tailwindcss": "^0.5.6",
        "tailwindcss": "^3.3.3"
    }
}
